{
    "extends": [
        "config:base",
        ":automergeMinor",
        ":automergeDigest"
    ],
    "enabledManagers": [
        "dockerfile",
        "gomod"
    ],
    "dockerfile": {
        "fileMatch": [
            "Dockerfile"
        ],
        "pinDigests": true
    },
    "gomod": {
        "postUpdateOptions": [
            "gomodTidy"
        ]
    },
    "labels": [
        "renovate"
    ]
}
